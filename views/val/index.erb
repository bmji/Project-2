<h1>Valorant Agent Roster</h1>

<% if logged_in? %>
    <h2><%= current_user['name'] %></h2>

    <form action="/sessions" method="POST">
        <input type="hidden" name="_method" value="delete">

        <button>Log Out</button>
    </form>
<% else %>
    <a href="/sign_up">Sign up</a>
    <a href="/login">Sign in</a>
<% end %>
